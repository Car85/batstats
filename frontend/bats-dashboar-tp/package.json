{
  "name": "bats-dashboar-tp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Car85",
  "description": "BatStats is a dynamic and user-friendly solution for analyzing and visualizing data.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "start": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-pack": "electron-builder",
    "build-electron": "electron-builder --linux AppImage"
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "exceljs": "^4.4.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "papaparse": "^5.5.2",
    "plotly.js": "^2.35.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-pivottable": "^0.10.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^7.1.1",
    "simple-statistics": "^7.8.7",
    "uuid": "^11.0.3",
    "wait-on": "^8.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.1",
    "@types/papaparse": "^5",
    "@types/plotly.js": "^2.35.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-pivottable": "^0.11.6",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^34.1.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "ts-loader": "^8.1.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "webpack": "4.46.0",
    "webpack-cli": "3",
    "webpack-dev-server": "3"
  },
  "resolutions": {
    "vite": "^6.0.6",
    "regl": "2.1.0",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3"
  },
  "build": {
    "extraMetadata": {
      "main": "dist/src/main.js"
    },
    "files": "./electron/main.ts",
    "appId": "com.car85.batsats",
    "productName": "BatStats",
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "../../icon/icon_batstats_2.png",
      "category": "Utility",
      "maintainer": "Car85 <car85@startmail.com>",
      "description": "BatStats is a dynamic and user-friendly solution for analyzing and visualizing data."
    }
  },
  "packageManager": "yarn@4.6.0",
  "main": "./dist/src/main.js"
}
